{
    "version": "0.2.0",
    "configurations": [{
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "sourceMaps": true,
        "program": "${workspaceFolder}/src/example/example.ts",
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "preLaunchTask": "tsc: build - tsconfig.json",
    }]
}